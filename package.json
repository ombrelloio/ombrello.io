{
  "name": "ombrello-nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint --fix --quiet './src/**/*.{ts,tsx}'",
    "format": "prettier --write './src/**/*.{ts,tsx}'",
    "export": "next export",
    "build:icons": "npx @svgr/cli --index-template .svgr/icon-template.js --typescript --icon --replace-attr-values '#544B70=currentColor' -d src/components/atoms/Icon/icons src/assets/SVG && mv src/components/atoms/Icon/icons/index.ts src/components/atoms/Icon/Icon.tsx"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "yarn lint"
    ],
    "src/**/*.{json,css,scss,sass}": [
      "yarn format"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@types/memory-cache": "^0.2.2",
    "@types/react-transition-group": "^4.4.4",
    "classnames": "^2.3.1",
    "date-fns": "^2.11.1",
    "dotenv-flow": "^3.2.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-filenames": "^1.3.2",
    "graphql": "^16.3.0",
    "graphql-request": "^4.2.0",
    "gray-matter": "^4.0.3",
    "gsap": "^3.8.0",
    "i18next": "^21.6.13",
    "js-yaml-loader": "^1.2.2",
    "memory-cache": "^0.2.0",
    "next": "^12.1.5",
    "next-seo": "^5.1.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-html-map": "^0.1.1",
    "react-i18next": "^11.14.2",
    "sass": "^1.34.0",
    "swiper": "^8.3.2",
    "swr": "^1.3.0",
    "tailwind-safelist-generator": "^0.1.3",
    "usehooks-ts": "^2.6.0"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^12.1.0",
    "@svgr/webpack": "5.5.0",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/forms": "^0.5.0",
    "@tailwindcss/line-clamp": "^0.3.0",
    "@tailwindcss/typography": "^0.5.0",
    "@types/node": "^17.0.21",
    "@types/react": "^18.0.5",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "autoprefixer": "^10.4.0",
    "css-loader": "^6.7.0",
    "eslint": "^8.11.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript-prettier": "^5.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.5",
    "postcss": "^8.4.8",
    "prettier": "^2.5.1",
    "react-transition-group": "^4.4.2",
    "sass-loader": "^12.3.0",
    "style-loader": "^3.3.1",
    "tailwindcss": "^3.1.8",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "typescript": "^4.5.2"
  }
}
